<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mapbox.mapboxandroiddemo.examples.extrusions.MarathonExtrusionActivity">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="33.334915"
        mapbox:mapbox_cameraTargetLng="-118.335074"
        mapbox:mapbox_cameraZoom="12.692151"
        mapbox:mapbox_cameraTilt="55.8873"
        mapbox:mapbox_styleUrl="@string/mapbox_style_satellite"/>

</android.support.constraint.ConstraintLayout>
