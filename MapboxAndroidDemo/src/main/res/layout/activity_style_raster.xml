<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".examples.styles.CustomRasterStyleActivity">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="59.31"
        mapbox:mapbox_cameraTargetLng="18.06"
        mapbox:mapbox_styleUrl="https://www.mapbox.com/android-docs/files/mapbox-raster-v8.json"
        mapbox:mapbox_cameraZoom="9"/>

</RelativeLayout>